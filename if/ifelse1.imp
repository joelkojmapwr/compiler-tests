PROGRAM IS
a,b,c
IN
    a:=5;
    IF a > 3 THEN
        WRITE a;
        a:=20;
    ENDIF

    IF a < 15 THEN
        WRITE a; # Should not be executed
    ELSE
        WRITE 0;    
    ENDIF

    IF a = 20 THEN
        a:=25;
        WRITE a;
    ELSE
        WRITE 1; # Should not be executed
    ENDIF
END