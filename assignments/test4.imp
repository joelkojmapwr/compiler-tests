PROGRAM IS
    a[1:3], i
IN
    i := 1;
    a[i] := 5;      # a[1] = 5
    WRITE a[i];     # 5
    WRITE a[1];     # 5
   
    i := 2;
    a[i] := 7;      # a[2] = 7
    WRITE a[i];     # 7
    WRITE a[2];     # 7
   
    i := 3;
    a[i] := a[1] + a[2];  # a[3] = 5 + 7 = 12
    WRITE a[i];     # 12
    WRITE a[3];     # 12
END